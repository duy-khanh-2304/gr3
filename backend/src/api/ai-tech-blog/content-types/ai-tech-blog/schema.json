{
  "kind": "collectionType",
  "collectionName": "ai_tech_blogs",
  "info": {
    "singularName": "ai-tech-blog",
    "pluralName": "ai-tech-blogs",
    "displayName": "AI Tech Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "publishAt": {
      "type": "datetime",
      "required": true
    },
    "post_subtitle": {
      "type": "richtext",
      "required": true
    },
    "showCommunicationLink": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "showCommentBox": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.paragraph-with-title",
        "content.intro-team",
        "content.paragraph",
        "content.pre-formatted-paragraph"
      ]
    },
    "post_image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    }
  }
}
