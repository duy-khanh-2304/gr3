{
  "kind": "collectionType",
  "collectionName": "news_and_events",
  "info": {
    "singularName": "news-and-event",
    "pluralName": "news-and-events",
    "displayName": "News and Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "publishAt": {
      "type": "datetime"
    },
    "post_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "post_subtitle": {
      "type": "richtext",
      "required": true
    },
    "tag": {
      "type": "customField",
      "options": [
        "news",
        "events",
        "seminars"
      ],
      "required": true,
      "customField": "plugin::multi-select.multi-select"
    },
    "showCommunicationLink": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "showCommentBox": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.intro-team",
        "content.paragraph",
        "content.pre-formatted-paragraph"
      ],
      "required": true
    }
  }
}
