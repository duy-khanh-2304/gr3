{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "singularName": "home-page",
    "pluralName": "home-pages",
    "displayName": "Home Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "header": {
      "displayName": "Header",
      "type": "component",
      "repeatable": false,
      "component": "page.header"
    },
    "nav_bar": {
      "type": "dynamiczone",
      "components": [
        "page.logo-navigation",
        "page.links-navigation",
        "page.dropdown-navigation"
      ]
    },
    "footer": {
      "type": "dynamiczone",
      "components": [
        "page.logo-navigation",
        "page.section-r-and-d-footer",
        "page.section-news-and-seminars-footer",
        "page.section-information-footer"
      ]
    },
    "absolute_footer": {
      "displayName": "Absolute Footer",
      "type": "component",
      "repeatable": false,
      "component": "page.absolute-footer"
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "page.section",
        "page.banner"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    },
    "menu": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::menus.menu"
    }
  }
}
