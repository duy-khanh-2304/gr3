{
  "kind": "collectionType",
  "collectionName": "newses",
  "info": {
    "singularName": "news",
    "pluralName": "newses",
    "displayName": "News",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "showCommunicationLink": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "showCommentBox": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "post_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "publishAt": {
      "type": "datetime",
      "required": true
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "tag": {
      "type": "customField",
      "options": [
        "news",
        "event",
        "seminar"
      ],
      "customField": "plugin::multi-select.multi-select"
    },
    "post_subtitle": {
      "type": "richtext",
      "required": true
    }
  }
}
