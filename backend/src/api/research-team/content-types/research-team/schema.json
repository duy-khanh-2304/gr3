{
  "kind": "collectionType",
  "collectionName": "research_teams",
  "info": {
    "singularName": "research-team",
    "pluralName": "research-teams",
    "displayName": "Research Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "post_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "introduction": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "research_directions": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "research_problems": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "members": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::member.member",
      "inversedBy": "research_teams"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    },
    "tool_and_resources": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tool-and-resource.tool-and-resource"
    },
    "slug": {
      "type": "uid"
    },
    "post_subtitle": {
      "type": "string",
      "required": true
    },
    "collaborations": {
      "type": "customField",
      "options": {
        "preset": "toolbar"
      },
      "required": false,
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "publications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication-list.publication-list"
    },
    "solutions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::solution.solution"
    },
    "seen_time_array": {
      "type": "component",
      "repeatable": true,
      "component": "content.seen-array-time"
    }
  }
}
